{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "/victoria-metrics-prod -storageDataPath=/data -httpListenAddr=:8428 -retentionPeriod=${RETENTION_PERIOD:-1} -search.latencyOffset=${SEARCH_LATENCY_OFFSET:-30s} -search.maxUniqueTimeseries=${MAX_UNIQUE_TIMESERIES:-300000}",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "volumes": ["/data"]
  }
}
